extends layout

block content
  .container
  form#contact(action='', method='post')
    h3 Registrate
    fieldset
      input(placeholder='Nombre', type='text', tabindex='1', required='', autofocus='')
    fieldset
      input(placeholder='Email', type='email', tabindex='2', required='')
    fieldset
      input(placeholder='Contraseña', type='password', tabindex='2', required='')
    fieldset
      input(placeholder='Confirmar contraseña', type='password', tabindex='2', required='')
    fieldset
      button#contact-submit(name='submit', type='submit', data-submit='..Enviando') Register

  
